{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./components/*"],
      "@types/*": ["../../types/*"],
      "#imports": [".nuxt/imports.d.ts"],
      "#ui/types": ["./node_modules/@nuxt/ui/dist/runtime/types"],
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
    },
  }
}
